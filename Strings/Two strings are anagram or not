
        if(a.length()!=b.length()){
            return false;
        }
        
        char[] c1=a.toCharArray();
        char[] c2=b.toCharArray();
        Arrays.sort(c1);
        Arrays.sort(c2);
        for(int i=0;i<c1.length;i++){
            if(c1[i]!=c2[i])
            return false;
        }
        return true;
