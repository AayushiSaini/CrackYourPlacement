        System.out.print(str.contains("aini"));
        System.out.println("");
        System.out.print(str.contains("gik"));
         System.out.println("");
          System.out.print(str.toLowerCase());
           System.out.println("");
           System.out.print(str.toUpperCase());
            System.out.println("");
             System.out.print(str.concat(s));
              System.out.println("");
              str=str+" "+"KIET GROUPS";
               System.out.println(str);
        
